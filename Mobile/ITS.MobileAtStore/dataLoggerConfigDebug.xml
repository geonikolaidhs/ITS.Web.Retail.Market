<Settings>
  <!--The folder that we like the items.db to be saved-->
  <LocalItemsDBLocation>\Program Files\WRM Mobile@Store\</LocalItemsDBLocation>
  <!-- The WebService Server IP-->
  <ServerIP>10.3.1.151/webclient</ServerIP>
  <!--The Operation Mode of the mobile APP. 1 for Batch, 2 for Online. This currently affects Only the price checking-->
  <OperationMode>2</OperationMode>
  <!-- To compcode εδώ χρησιμοποιείται επίσης και στον έλεγχο προσφορών. Αφήστε το κενό για εγκαταστάσεις SRS που έχουν μόνο 1 υποκατάστημα-->
  <AdvancedPriceChecking Active="True">
    <CompCode>4</CompCode>
    <PriceCatalogPolicy>3</PriceCatalogPolicy>
  </AdvancedPriceChecking>
  <EAN128BarcodeSettings Active="True">
    <IdentifierSetting IdentifierType="productcodeunit" IdentifierTypeString="01" TruncateLeft="1" VariableWeightText="9" ConstantWeightText="0" ConstantContentLength="14"/>
    <IdentifierSetting IdentifierType="productcodevariable" IdentifierTypeString="02" TruncateLeft="1" VariableWeightText="9" ConstantWeightText="0" ConstantContentLength="14"/>
    <IdentifierSetting IdentifierType="quantityunits" IdentifierTypeString="37" MaxContentLength="8"/>
    <IdentifierSetting IdentifierType="quantityweight" IdentifierTypeString="310" ConstantContentLength="7"/>
    <IdentifierSetting IdentifierType="lot" IdentifierTypeString="10" MaxContentLength="20"/>
    <IdentifierSetting IdentifierType="expirationdate" IdentifierTypeString="17" ConstantContentLength="6"/>
  </EAN128BarcodeSettings>

  <!-- Ορίζουμε τα document types χωρισμένα με κόμμα πχ(ORDER,PICKING) στα οποία θέλουμε να ισχύουν τα παρακάτω settings-->
