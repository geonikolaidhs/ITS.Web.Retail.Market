<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tabCustomerInfo"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <ScrollView
        android:id="@+id/scrollView123634345"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="10dp" >

        <LinearLayout
            android:id="@+id/ll143sf11334"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingLeft="10dp"
            android:paddingRight="10dp" >

            <TableLayout
                android:id="@+id/ll111340custinfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal" >

                <TableRow
                    android:id="@+id/tableRow10custinfo"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/textView323231custinfo"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right|center_vertical"
                        android:text="@string/code"
                        android:textAppearance="?android:attr/textAppearanceLarge" />

                    <EditText
                        android:id="@+id/txtCustomerCode"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:editable="false"
                        android:ems="10"
                        android:inputType="none" />

                    <TextView
                        android:id="@+id/textView323232231custinfo"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right|center_vertical"
                        android:text="@string/taxCode"
                        android:textAppearance="?android:attr/textAppearanceLarge" />

                    <EditText
                        android:id="@+id/txtCustomerTaxCode"
                        android:layout_width="fill_parent"
                        android:editable="false"
                        android:ems="10"
                        android:inputType="none" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow11custinfo"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/textView32323223123custinfo"
                        android:gravity="right|center_vertical"
                        android:text="@string/companyName"
                        android:textAppearance="?android:attr/textAppearanceLarge" />

                    <EditText
                        android:id="@+id/txtCustomerCompanyName"
                        android:layout_span="3"
                        android:editable="false"
                        android:ems="10"
                        android:inputType="none" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow15custinfo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/textView32444563534523211231custinfo"
                        android:layout_width="fill_parent"
                        android:gravity="right|center_vertical"
                        android:text="@string/txtPriceCatalog"
                        android:textAppearance="?android:attr/textAppearanceLarge" />

                    <EditText
                        android:id="@+id/txtCustomerPriceCatatlog"
                        android:layout_width="fill_parent"
                        android:layout_span="3"
                        android:editable="false"
                        android:ems="10"
                        android:inputType="none" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow12custinfo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/textView32323211231custinfo"
                        android:layout_width="fill_parent"
                        android:gravity="right|center_vertical"
                        android:text="@string/DefaultAddress"
                        android:textAppearance="?android:attr/textAppearanceLarge" />

                    <EditText
                        android:id="@+id/txtCustomerDefaultAddress"
                        android:layout_width="fill_parent"
                        android:layout_span="3"
                        android:editable="false"
                        android:ems="10"
                        android:inputType="none" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow13acustinfo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/textView32442132311231custinfo"
                        android:layout_width="fill_parent"
                        android:gravity="right|center_vertical"
                        android:text="@string/Phone"
                        android:textAppearance="?android:attr/textAppearanceLarge" />

                    <EditText
                        android:id="@+id/txtCustomerDefaultPhone"
                        android:layout_width="fill_parent"
                        android:layout_span="2"
                        android:editable="false"
                        android:ems="10"
                        android:inputType="none" />

                    <Button
                        android:id="@+id/btnCheckPrice"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/checkPrice" />
                </TableRow>
            </TableLayout>

            <LinearLayout
                android:id="@+id/dummyid346221137"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <Button
                    android:id="@+id/btnLoadHeaders"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/customerTab" />

                <CheckBox
                    android:id="@+id/chkUseFilters"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checked="true"
                    android:text="@string/useFilters" />
            </LinearLayout>

            <TableLayout
                android:id="@+id/filtersTable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:stretchColumns="*"
                android:visibility="visible" >
                 <TableRow
                    android:id="@+id/tableRow10custinfo"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/textView323231custinfo"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_span="4"
                        android:gravity="left|center_vertical"
                        android:text="@string/DateFinalization"
                        android:textAppearance="?android:attr/textAppearanceLarge" />

                </TableRow>

                <TableRow
                    android:id="@+id/tableRow334"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/TextView12234255"
                        android:layout_width="wrap_content"
                        android:gravity="right|center_vertical"
                        android:text="@string/FromFilter"
                        android:textAppearance="?android:attr/textAppearanceLarge" />

                    <DatePicker
                        android:id="@+id/dtFromDateFilter"
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:calendarViewShown="false" />

                    <TextView
                        android:id="@+id/TextView122234934255"
                        android:layout_width="wrap_content"
                        android:gravity="right|center_vertical"
                        android:text="@string/ToFilter"
                        android:textAppearance="?android:attr/textAppearanceLarge" />

                    <DatePicker
                        android:id="@+id/dtToDateFilter"
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:calendarViewShown="false" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow335"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/TextView12223324984934255"
                        android:layout_width="fill_parent"
                        android:gravity="right|center_vertical"
                        android:text="@string/StatusFilter"
                        android:textAppearance="?android:attr/textAppearanceLarge" />

                    <Spinner
                        android:id="@+id/cmbStatusFilter"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/TextView01452224ftf24"
                        android:layout_width="fill_parent"
                        android:gravity="right|center_vertical"
                        android:text="@string/TypeFilter"
                        android:textAppearance="?android:attr/textAppearanceLarge" />

                    <Spinner
                        android:id="@+id/cmbTypeFilter"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow336"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/TextView3546745730200"
                        android:layout_width="fill_parent"
                        android:gravity="right|center_vertical"
                        android:text="@string/HasBeenCheckedFilter"
                        android:textAppearance="?android:attr/textAppearanceLarge" />

                    <Spinner
                        android:id="@+id/cmbHasBeenCheckedFilter"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/TextView53702343677"
                        android:layout_width="fill_parent"
                        android:gravity="right|center_vertical"
                        android:text="@string/HasBeenExecutedFilter"
                        android:textAppearance="?android:attr/textAppearanceLarge" />

                    <Spinner
                        android:id="@+id/cmbHasBeenExecutedFilter"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />
                </TableRow>
            </TableLayout>

            <include
                android:id="@+id/documentHeaderColumns"
                layout="@layout/document_header_row"
                android:visibility="gone" />

            <ListView
                android:id="@+id/lstDocumentHeaders"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone" >
            </ListView>
        </LinearLayout>

    </ScrollView>

</FrameLayout>