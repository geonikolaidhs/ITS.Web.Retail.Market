@Html.DevExpress().CallbackPanel(cpb_settings =>
{
    cpb_settings.Name = "ResetPasswordCallbackPanel";
    cpb_settings.CallbackRouteValues = new { Controller = "User", Action = "ResetPasswordCallbackPanel" };
    cpb_settings.SetContent(() =>
    {
        Html.RenderPartial("ResetPasswordForm");
    });
}).GetHtml()
